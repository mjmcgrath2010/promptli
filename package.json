{
  "name": "promptli",
  "version": "1.0.0",
  "devDependencies": {
    "husky": "^4.3.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "docker-compose up",
    "client:bash": "docker exec -it client bash",
    "server:bash": "docker exec -it api bash",
    "heroku-postbuild": "cd client && yarn install --production=false && yarn build"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjmcgrath2010/promptli-app.git"
  },
